import axios from 'axios';
import Parse from 'appdir/api';
import { restHeaders } from "appdir/app";


const fetch = () => {
    let hdr = restHeaders();
    return axios.get(`/some/rest/route`, {headers: hdr}).then(({data}) => data);
};

const fetchParse = () => {
    let hdr = restHeaders();
    return Parse.Cloud.run('some-function');
};

export default {
    fetch,
    fetchParse,
};
